{
  "name": "uuGle",
  "version": "0.0.8",
  "description": "uuGle Chrome extension provides fast and user-friendly searching across Unicorn bookkit and dockit books in browser search bar.",
  "omnibox": { "keyword" : "uu" },
  "background": {
    "scripts": ["background.bundle.js"]
  },
  "permissions": [ "tabs", "https://*.plus4u.net/uu-bookkitg01-main/*", "https://*.plus4u.net/uu-dockitg01-main/*", "https://*.plus4u.net/uu-bookkit-maing01/*" ],
  "icons": {
    "34": "icon-34.png",
    "128": "icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "34": "icon-34.png"
    },
    "default_title": "Click Me",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["https://*.plus4u.net/uu-bookkitg01-main/*", "https://*.plus4u.net/uu-dockitg01-main/*", "https://*.plus4u.net/uu-bookkit-maing01/*"],
    "js": ["contentScript.bundle.js"],
    "css": ["content.styles.css"]
  }],
  "web_accessible_resources": [
    "inject.bundle.js",
    "content.styles.css",
    "icon-128.png",
    "icon-34.png"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}